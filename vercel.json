{
  "framework": "vitepress",
  "buildCommand": "npm run docs:build",
  "installCommand": "npm install",
  "outputDirectory": ".vitepress/dist",
  "trailingSlash": false,
  "routes": [
    {
      "src": "/(.*)",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" },
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true
}